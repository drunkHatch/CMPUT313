compile = "saw.c"

bandwidth = 56Kbps,
messagerate = 1000ms,
propagationdelay = 2500ms,

minmessagesize = 2000bytes,
maxmessagesize = 4000bytes,

probframeloss = 2,
probframecorrupt = 2,

host Perth {
    x=100, y=300,
    address=10
    link to r1
}
router r1 {
    east of Perth
    address=11
    link to r2
}
router r2 {
    east of r1
    address=12
    link to r3
}
router r3 {
    east of r2
    address = 13
    link to Sydney
}
host Sydney {
     east of r3
     address = 14
}
